MODULE user_command_0100 INPUT.

  CASE sy-ucomm.
    WHEN 'EXIT' OR 'CNCL'.
      LEAVE PROGRAM.
    WHEN 'BACK'.
      LEAVE TO SCREEN 0.
    WHEN OTHERS.
  ENDCASE.


ENDMODULE.
